<?xml version="1.0" encoding="UTF-8"?>
<VehicleObjectXMLList>

	<!-- FOR ONE VEHICLE OBJECT -->
	<VehicleObject>
		<!-- Graphic Properties -->
		<Name></Name> <!-- User identification -->
		<TypeName></TypeName> <!-- Mapping Identification -->
		<Directory></Directory>
		
		<!-- Physcial Properties -->
		<IsPlayable></IsPlayable>
		<NumberPerSquad></NumberPerSquad>
		<Type></Type> <!-- 1 treaded tanks, 2 wheel vehicles, 3 civilian vehicles -->
		<Speed></Speed>
		<Armor></Armor>
		<HitPoints></HitPoints>
		<TurnRate></TurnRate>
		<Weight></Weight> <!-- Used to Determine, how far it flies when hit -->
		<HasTurrent></HasTurrent> <!-- If yes, look at TurrentProperties, else WeaponProperties -->
		<Size> <!-- Overall Size, should surround entire unit -->
			<Height></Height> <!-- For drawing and collision detection, helicopter collisions will be based on object height -->
			<Width></Width>
			<Radius></Radius> <!-- Radius A circle around unit for collision detection (pathfinding wise) -->
		</Size>
		
		<!-- Weapon Properties -->
		<!-- TWO POSSIBILITIES -->
		<!-- 1.) Without Turret -->
		<WeaponProperties>
			<Weapon></Weapon> <!-- What weapon type it is, defined in another xml -->
		</WeaponProperties>
		<!-- 2.) With Turret -->	
		<WeaponProperties>
			<TurretProperties>
				<Type></Type> <!-- Which Turret Type -->
				<RelativePosition> <!-- Relative Position of Turrent to body -->
					<RelativeXCoord></RelativeXCoord>
					<RelativeYCoord></RelativeYCoord>
				</RelativePosition>
			</TurretProperties>
			<Weapon></Weapon> <!-- What weapon type it is, defined in another xml -->
		</WeaponProperties>
	</VehicleObject>

</VehicleObjectXMLList>
